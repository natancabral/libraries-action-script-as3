						/*if(gnncKeyboard.__ENTER(event))
						{
						focusManager.getNextFocusManagerComponent().setFocus();
						focusManager.getNextFocusManagerComponent().drawFocus(true);
						
						focusManager.showFocus();
						focusManager.getFocus();
						focusManager.activate();
						}*/

				//PRODUCT_.__clear();
				PRODUCT_.selectedIndex = -1;
				__productListInAndOut();
				_change = true;
				return;
				_gnncPopUp.__close();
				__close();
			}
			
			protected function __fFault(event:Fault):void
			{
				_gnncPopUp.__close();
			}
			

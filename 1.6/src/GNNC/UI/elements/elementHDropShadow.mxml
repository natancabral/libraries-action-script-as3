<?xml version="1.0" encoding="utf-8"?>
<s:Rect xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx"
		width="100%" height="4"
		>
	<fx:Declarations>
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			
			public function set __inverse(value_:Boolean):void
			{
				A.color = 0x555555;
				A.alpha = 0;

				B.color = 0x333333;
				B.alpha = 0.5;
			}
			
		]]>
	</fx:Script>
	<s:fill>
		<s:LinearGradient id="C" rotation="90">
			<s:GradientEntry id="A" color="0x333333" alpha=".5"/>
			<s:GradientEntry id="B" color="0x555555" alpha="0"/>
		</s:LinearGradient>
	</s:fill>
</s:Rect>
